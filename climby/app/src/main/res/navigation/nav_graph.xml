<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment2">

    <fragment
        android:id="@+id/registerFragment"
        android:name="com.climby.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
    </fragment>
    <fragment
        android:id="@+id/myProfileFragment"
        android:name="com.climby.MyProfileFragment"
        android:label="fragment_my_profile"
        tools:layout="@layout/fragment_my_profile" />
    <fragment
        android:id="@+id/myRoutesFragment"
        android:name="com.climby.MyRoutesFragment"
        android:label="fragment_my_routes"
        tools:layout="@layout/fragment_my_routes" />
    <fragment
        android:id="@+id/myClimbsFragment"
        android:name="com.climby.MyClimbsFragment"
        android:label="fragment_my_climbs"
        tools:layout="@layout/fragment_my_climbs" />
    <fragment
        android:id="@+id/routeDetailFragment"
        android:name="com.climby.RouteDetailFragment"
        android:label="fragment_route_detail"
        tools:layout="@layout/fragment_route_detail" />
    <fragment
        android:id="@+id/climbDetailFragment"
        android:name="com.climby.ClimbDetailFragment"
        android:label="fragment_climb_detail"
        tools:layout="@layout/fragment_climb_detail" />
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.climby.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/loginFragment2"
        android:name="com.climby.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment2_to_mapsFragment"
            app:destination="@id/mapsFragment" />
        <action
            android:id="@+id/action_loginFragment2_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
</navigation>